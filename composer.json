{
    "name": "bartechdev/craft-dashboard-themes",
    "description": "This plugin allows to change dashboard theme.",
    "type": "craft-plugin",
    "license": "proprietary",
    "support": {
        "email": "contact@bar-tech.dev"
    },
    "require": {
        "php": ">=8.0.2",
        "craftcms/cms": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "bartechdev\\craftdashboardthemes\\": "src/"
        }
    },
    "extra": {
        "handle": "dashboard-themes",
        "name": "Dashboard Themes",
        "developer": "Bartlomiej Witkowski",
        "documentationUrl": ""
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}

